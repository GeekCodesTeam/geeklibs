<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geek.libglide47">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_namelibglide47"
        android:supportsRtl="true">
        
        <activity android:name=".GlideMainActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.GlideMainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".GlideMainActivityalluse"
            android:exported="false">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.GlideMainActivityalluse" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GlideMainActivityjiugongge"
            android:exported="false"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.GlideMainActivityjiugongge" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demo.imageutil.SingleImageActivity"
            android:exported="false"
            android:theme="@style/AppCompatFullscreenStyle" />
        <activity
            android:name=".demo.imageutil.SingleImagePhotoViewActivity"
            android:exported="false"
            android:theme="@style/AppCompatFullscreenStyle" />
        <activity
            android:name=".demo.imageutil.ImagesActivity"
            android:exported="false"
            android:theme="@style/ImagesStyle" />
    </application>

</manifest>
