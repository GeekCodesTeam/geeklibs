<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/search_blue1" />
            <solid android:color="@color/search_blue1" />
            <corners android:radius="40mm" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/search_blue2" />
            <solid android:color="@color/search_blue2" />
            <corners android:radius="40mm" />
        </shape>
    </item>
</selector>