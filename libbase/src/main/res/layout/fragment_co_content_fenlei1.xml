<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <com.geek.tablib.tablayout.SlidingTabLayout
        android:id="@+id/tl_fenlei1"
        android:layout_width="match_parent"
        android:layout_height="40mm"
        tl:tl_indicator_color="#E50D02"
        tl:tl_indicator_corner_radius="1.5mm"
        tl:tl_indicator_height="3mm"
        tl:tl_indicator_width="30mm"
        tl:tl_textSelectColor="#E50D02"
        tl:tl_textUnselectColor="#333333"
        tl:tl_underline_color="#E50D02"
        tl:tl_underline_height="0mm" />

    <com.geek.libbase.widgets.ViewPagerSlide
        android:id="@+id/vs_fenlei1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
