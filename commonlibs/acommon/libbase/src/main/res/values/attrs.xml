<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmartBar">
        <attr name="home_icon" format="reference" />
        <attr name="back_icon" format="reference" />
        <attr name="icon_padding" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="loading_layout_img_an" format="integer|reference" />
        <attr name="loading_layout" format="reference" />
        <attr name="nodata_layout" format="reference" />
        <attr name="errornet_layout" format="reference" />
        <attr name="unreachable_layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="LxLayout">
        <attr name="is_touch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AlphaView">
        <attr name="pointvisbile">
            <enum name="visible" value="0" />
            <enum name="gone" value="8" />
        </attr>

        <attr name="pointgravity">
            <enum name="left" value="8" />
            <enum name="right" value="5" />
            <enum name="center" value="17" />
            <enum name="center_horizontal" value="1" />
            <enum name="center_vertical" value="16" />
        </attr>
    </declare-styleable>

</resources>